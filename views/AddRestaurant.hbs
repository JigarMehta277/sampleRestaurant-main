    <body>
    <h1>Insert Restaurant</h1>

    {{#if message}}
        <p>{{message}}</p>
    {{/if}}

    <form action="/api/restaurants" method="post">
        <label for="name">Name:</label>
        <input type="text" id="name" name="name" required>

        <label for="cuisine">Cuisine:</label>
        <input type="text" id="cuisine" name="cuisine" required>

        <label for="building">Building:</label>
        <input type="text" id="building" name="address.building" required>

        <label for="coord0">Coordinate 1:</label>
        <input type="number" id="coord0" name="address.coord[0]" required>

        <label for="coord1">Coordinate 2:</label>
        <input type="number" id="coord1" name="address.coord[1]" required>

        <label for="street">Street:</label>
        <input type="text" id="street" name="address.street" required>

        <label for="zipcode">Zipcode:</label>
        <input type="text" id="zipcode" name="address.zipcode" required>

        <label for="borough">Borough:</label>
        <input type="text" id="borough" name="address.borough" required>

        <label for="gradeDate">Grade Date:</label>
        <input type="text" id="gradeDate" name="grades[0].date" required>

        <label for="grade">Grade:</label>
        <input type="text" id="grade" name="grades[0].grade" required>

        <label for="score">Score:</label>
        <input type="number" id="score" name="grades[0].score" required>

        <button type="submit">Insert Restaurant</button>
    </form>

</body>